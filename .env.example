# Notion API Configuration
# Copy this file to .env and fill in your actual values

# Your Notion integration token (starts with 'secret_')
NOTION_TOKEN=secret_your_token_here

# Your Meetings database ID (from the database URL)
DATABASE_ID=your_meetings_database_id_here

# Your People database ID (optional, for automatic people linking)
PEOPLE_DATABASE_ID=your_people_database_id_here

# Your Projects database ID (optional, for automatic project linking)
PROJECTS_DATABASE_ID=your_projects_database_id_here

# OpenAI API key (for transcript processing)
OPENAI_API_KEY=sk-your_openai_api_key_here
